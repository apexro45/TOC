#include <stdio.h>
#include <string.h>

#define MAX_LEN 100

int main() {
    char input[MAX_LEN];
    printf("Enter the first string (aaa): ");
    scanf("%s", input);
    int len1 = strlen(input);
    if (len1 != 3 || input[0] != 'a' || input[1] != 'a' || input[2] != 'a') {
        printf("Invalid first string\n");
        return 0;
    }
    printf("Enter the second string (aa): ");
    scanf("%s", input);
    int len2 = strlen(input);
    if (len2 != 2 || input[0] != 'a' || input[1] != 'a') {
        printf("Invalid second string\n");
        return 0;
    }
    printf("Result: aa\n");
    return 0;
}
